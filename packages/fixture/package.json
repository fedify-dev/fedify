{
  "name": "@fedify/fixture",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fedify-dev/fedify/issues"
  },
  "funding": [
    "https://opencollective.com/fedify",
    "https://github.com/sponsors/dahlia"
  ],
  "type": "module",
  "main": "./dist/mod.cjs",
  "module": "./dist/mod.js",
  "types": "./dist/mod.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mod.js",
      "require": "./dist/mod.cjs",
      "default": "./dist/mod.js"
    },
    "./fixtures/*": "./dist/fixtures/*",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "package.json"
  ],
  "peerDependencies": {
    "@fedify/fedify": "workspace:^",
    "@fedify/vocab-runtime": "workspace:^",
    "@logtape/logtape": "catalog:",
    "@opentelemetry/core": "catalog:",
    "@opentelemetry/sdk-trace-base": "catalog:"
  },
  "devDependencies": {
    "tsdown": "catalog:",
    "typescript": "catalog:"
  },
  "scripts": {
    "build": "tsdown",
    "prepack": "tsdown",
    "prepublish": "tsdown",
    "test": "deno task codegen && tsdown && cd dist/ && node --test"
  }
}
