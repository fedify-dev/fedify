{
  "name": "@fedify/cli-optique",
  "version": "2.0.0",
  "license": "MIT",
  "exports": "./src/mod.ts",
  "imports": {
    "@optique/core": "jsr:@optique/core@^0.2.0",
    "@optique/run": "jsr:@optique/run@^0.2.0",
    "ora": "npm:ora@^8.2.0"
  },
  "tasks": {
    "codegen": "deno task -f @fedify/fedify codegen",
    "check": {
      "command": "deno task codegen && deno fmt --check && deno lint && deno check src/**/*.ts",
      "dependencies": [
        "codegen"
      ]
    },
    "run": {
      "command": "deno run --allow-all src/mod.ts",
      "dependencies": [
        "codegen"
      ]
    },
    "runi": "deno run --allow-all src/mod.ts"
  }
}
